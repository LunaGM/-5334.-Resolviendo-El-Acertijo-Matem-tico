#2360. E/S-Operaciones
a,b = map(int,input().split())
def Operaciones(a,b):
  A = Suma(a,b)
  B = resta(a,b)
  C = division(a,b)
  D = multiplicacion(a,b)
  E = modulo(a,b)
  print (A,B,C,D,E)
  return " "

def Suma(a,b):
  return a+b
def resta(a,b):
  return a-b
def division(a,b):
  return a//b
def multiplicacion(a,b):
  return a*b
def modulo(a,b):
  return a%b
  
print (Operaciones(a,b))
